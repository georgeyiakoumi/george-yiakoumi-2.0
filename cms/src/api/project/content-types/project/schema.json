{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "project_title": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true
    },
    "project_thumb": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "project_hero_image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "date": {
      "type": "date"
    },
    "tags": {
      "type": "customField",
      "customField": "plugin::tagsinput.tags"
    },
    "project_description": {
      "type": "text"
    },
    "challenge": {
      "type": "component",
      "repeatable": false,
      "component": "project-chapter.project-chapter"
    },
    "solution": {
      "type": "component",
      "repeatable": false,
      "component": "project-chapter.project-chapter"
    },
    "role": {
      "type": "component",
      "repeatable": false,
      "component": "project-chapter.project-chapter"
    },
    "impact": {
      "type": "component",
      "repeatable": false,
      "component": "project-chapter.project-chapter"
    },
    "takeaway": {
      "type": "component",
      "repeatable": false,
      "component": "project-chapter.project-chapter"
    },
    "tools": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::tool.tool"
    },
    "project_client": {
      "type": "string"
    },
    "project_role": {
      "type": "string"
    }
  }
}
